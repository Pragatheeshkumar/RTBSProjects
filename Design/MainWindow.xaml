                                    <GradientStop Color="#00719B" Offset="0.988" />
                                    <GradientStop Color="#FF5BD0E5" Offset="0.029" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center">

                            </ContentPresenter>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

