                                    </LinearGradientBrush>
                                </Border.Background>
                                <ContentPresenter  Margin="4" ContentSource="Header" RecognizesAccessKey="True" />
                            </Border>
                            <Border  Grid.Row="1"  Background="White" BorderBrush="#FFF3F4F6" BorderThickness="1,0,1,1" CornerRadius="0,0,0,0" >
                                <ContentPresenter Margin="4" />
